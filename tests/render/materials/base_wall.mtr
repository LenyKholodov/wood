textures/base_wall/jimtest1
{
	qer_editorimage	textures/base_wall/boxpanel_ed.tga

/*
	{
		if ( parm7 == 0 )
		blend bumpmap
		map		textures/base_wall/boxpanel_local.tga
	}

	{
		if ( parm7 == 0 )
		blend diffusemap
		map		textures/base_wall/boxpanel_d.tga
	}

	{
		if ( parm7 == 0 )
		blend specularmap
		map		textures/base_wall/boxpanel_s.tga
	}

*/
	{
		if ( parm7 == 0 )
		map		textures/base_wall/boxpanel_s.tga
	}

	{
		if ( ( parm7 == 1 ) && ( (time-parm3) < 1 ) )
		map		lights/window.tga
		rgb (.3 * sintable [ (time-parm3 - 0.25) * .5 ]) +.7
	}

	{
		if ( ( parm7 == 1 ) && ( (time-parm3) >= 1 ) )
		map		textures/sfx/flame2.tga
	}	
}


