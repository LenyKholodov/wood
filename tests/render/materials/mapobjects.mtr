table fivestage { 1, 0, 0, 0, 0 }

models/mapobjects/teleporter/zot1
{
    noselfShadow
    noshadows	
	translucent
	surfaceparm discrete
	surfaceparm nonsolid
	surfaceparm noimpact

	qer_editorimage  models/mapobjects/teleporter/zot1.tga 

		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/zot1.tga
			rgb	elevenstage [ time * Parm3 + Parm4 + 0/ 5 ]
		}
	               	
		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/zot2.tga
			rgb	elevenstage [ time * Parm3 + Parm4 + 1.0 / 5 ]
		}
	               	
		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/zot3.tga
			rgb	elevenstage [ time * Parm3 + Parm4 + 2.0 / 5 ]
		}
	               	
		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/zot4.tga
			rgb	elevenstage [ time * Parm3 + Parm4 + 3.0 / 5 ]
		}
	               	
		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/zot5.tga
			rgb	elevenstage [ time * Parm3 + Parm4 + 4.0 / 5 ]
		}
	               	

}


