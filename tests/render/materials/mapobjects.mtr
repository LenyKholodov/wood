models/mapobjects/teleporter/redflare

{
    noselfShadow
    noshadows	
	deform sprite
	translucent
	surfaceparm discrete
	surfaceparm nonsolid
	surfaceparm noimpact

	qer_editorimage  models/mapobjects/teleporter/redflare.tga 

		{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/redflare.tga
			rgb		eyestable[ time * 1 ] 
			rotate	( time * 6 )
	}
			{ 
      
      		 blend	add
	 		map	models/mapobjects/teleporter/redflare.tga
			rgb		eyestable[ time * 1.1 ] 
			rotate	( time * -5.3 )
	}
	      
	               	

}

