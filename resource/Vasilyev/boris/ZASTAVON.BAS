10 cls
11 OPEN "TSINGL" FOR INPUT AS#1
	INPUT #1,A
	SPD=A
	CLOSE #1
	COLOR 2
12 LOCATE 10,10:PRINT SPD
	FOR T=1 TO 5000000:NEXT T
14 CLS

15 FOR Y=1 TO 23
	FOR X=1 TO 76 STEP 4
	D=INT(RND*1000)
	LOCATE Y,X:PRINT D
	FOR T=1 TO SPD:NEXT T
	NEXT X
	NEXT Y

20 A$=INKEY$
	IF A$<>"" THEN END

30    STR=INT(RND*90)
	FOR Y=1 TO 24
	FOR X=1 TO 76 STEP 4
	ST=INT (RND*100)
	IF ST>STR THEN GOTO 35
	D=INT(RND*1000)
	LOCATE Y,X:PRINT D
35    FOR T=1 TO SPD:NEXT T
	NEXT X
	NEXT Y
	GOTO 20