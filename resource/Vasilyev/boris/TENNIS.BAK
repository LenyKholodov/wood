	IF A$="4" THEN KH=-45 :CHAN=CHAN+1
	IF A$="6" THEN KH=45  :CHAN=CHAN+1
	IF A$="7" THEN KH=-60 :CHAN=CHAN+1
	IF A$="9" THEN KH=60  :CHAN=CHAN+1
