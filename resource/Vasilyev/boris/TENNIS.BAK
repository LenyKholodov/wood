	IF XB<LT  AND AI<5 THEN XB=LT :XCH=-XCH:SOUND 400,0.8

	IF XB>RT-10 AND AI>=5 THEN XB=LT :SOUND 250,0.8
	IF XB<LT  AND AI>=5 THEN XB=RT-10:SOUND 250,0.8


	YB=YB+YCH
