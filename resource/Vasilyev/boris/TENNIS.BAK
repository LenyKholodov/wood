123   IF ABS(XCH)>ABS(MT) THEN RETURN
	IF XCH>0 THEN XCH=XCH+INT(RND*MT):RETURN
	IF XCH<0 THEN XCH=XCH-INT(RND*MT):RETURN
	XCH=INT(9*RND)-4
	RETURN

124   XCH=-XCH:RETURN

125   XCH=-XCH
	IF ABS(XCH)>MT THEN RETURN
	IF XCH<0 THEN XCH=XCH-1:RETURN
	IF XCH>0 THEN XCH=XCH+1:RETURN
	XCH=INT(RND*MT)-MT/2:RETURN

126   IF YCH<MT THEN YCH=YCH+1:RETURN
	RETURN

130   SP=INT(2*(ABS(RCH))/3 )
	MP=INT(2*(ABS(RCH))/5)
	BN=ABS(XCH)
	IF SP=0 THEN SP=3
	IF MP=0 THEN MP=2

	IF XB<KR-10 THEN GOTO 140
	IF XB>KR+60 THEN GOTO 140

	SOUND 400,0.8

	IF XB+5>=KR+20 AND XB+5<=KR+40  THEN YCH=-YCH:SC=SC+BN:GOTO 121
	IF XB+5>KR+40  AND XB+5<KR+60   THEN YCH=-YCH:XCH=XCH+MP:SC=SC+BN:GOTO 121
	IF XB+5<KR+20  AND XB+5>KR      THEN YCH=-YCH:XCH=XCH-MP:SC=SC+BN:GOTO 121

	IF XB+5 >=KR-10 AND XB+5<=KR+10 THEN YCH=-YCH:XCH=XCH-SP:SC=SC+BN:GOTO 121
	IF XB+5<=KR+60  AND XB+5>=KR+50 THEN YCH=-YCH:XCH=XCH+SP:SC=SC+BN:GOTO 121
	IF XB<KR+60     AND XB>KR+55    THEN YCH=-YCH:XCH=XCH+2*MP:SC=SC+BN:GOTO 121
	IF XB+10>KR     AND XB+10<KR+5  THEN YCH=-YCH:XCH=XCH-2*MP:SC=SC+BN:GOTO 121
140
	VB=1
